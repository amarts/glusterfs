[DEFAULT]
devices = /mnt/gluster-object
mount_check = true
bind_port = 6012    # Be sure to keep in sync with /usr/bin/gluster-swift-gen-builders
user = root
log_facility = LOG_LOCAL2

[pipeline:main]
pipeline = account-server

[app:account-server]
use = egg:swift#account

[account-replicator]
vm_test_mode = yes

[account-auditor]

[account-reaper]
